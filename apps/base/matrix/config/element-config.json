{
  "brand": "Brooke's Testing Instance",
  "default_theme": "dark",
  "default_server_name": "testing.borooka.ee",
  "default_server_config": {
    "m.homeserver": {
      "server_name": "testing.borooka.ee",
      "base_url": "https://matrix-testing.borooka.ee"
    }
  },
  "disable_guests": true,
  "disable_login_language_selector": false,
  "disable_3pid_login": false,
  "disable_custom_urls": true,
  "default_country_code": "EE",
  "show_labs_settings": false,
  "features": {
    "feature_video_rooms": true,
    "feature_group_calls": true,
    "feature_element_call_video_rooms": true,
    "feature_new_room_list": true
  },
  "element_call": {
    "use_exclusively": true,
    "url": "https://element-call-testing.borooka.ee"
  },
  "jitsi": {
    "preferred_domain": "meet-testing.borooka.ee",
    "skip_built_in_welcome_screen": true
  },
  "setting_defaults": {
    "use_system_theme": true,
    "MessageComposerInput.insertTrailingColon": true,
    "room_directory_servers": [
      "testing.borooka.ee"
    ],
    "MessageComposerInput.autoReplaceEmoji": true,
    "autoplayGifs": true,
    "autoplayVideo": true,
    "enableSyntaxHighlightLanguageDetection": true,
    "hideAvatarChanges": true,
    "hideDisplaynameChanges": true,
    "hideJoinLeaves": true,
    "layout": "bubble",
    "sendTypingNotifications": true,
    "showTypingNotifications": true,
    "notificationsEnabled": true,
    "audioNotificationsEnabled": true,
    "RoomList.preferredSorting": "Unread",
    "RoomList.showMessagePreview": false,
    "breadcrumbs": false,
    "Spaces.allRoomsInHome": false,
    "Spaces.enabledMetaSpaces": {
      "home-space": true,
      "orphans-space": true,
      "video-rooms-space": false,
      "home": false,
      "video_rooms": false,
      "people": true,
      "favourites": true
    },
    "Spaces.showPeopleInSpace": true,
    "Notifications.alwaysShowBadgeCounts": true,
    "userTimezonePublish": false
  },
  "room_directory": {
    "servers": ["testing.borooka.ee"]
  }
}
